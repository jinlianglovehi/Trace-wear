<?xml version="1.0" encoding="utf-8"?>
<!-- 开始采集数据界面按钮使用 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false"><shape>
        <solid android:color="@color/white_color" />
        <!-- 设置四个角的角度 -->
        <corners android:bottomLeftRadius="20dp" android:bottomRightRadius="20dp" android:topLeftRadius="20dp" android:topRightRadius="20dp" />
        <stroke android:width="2dp" android:color="@color/common_border_color" />
        <padding android:bottom="5dp" android:top="5dp" android:left="15dp" android:right="15dp"/>
    </shape></item>
    <item android:state_pressed="true"><shape>
        <solid android:color="@color/white_color" />
        <!-- 设置四个角的角度 -->
        <corners android:bottomLeftRadius="20dp" android:bottomRightRadius="20dp" android:topLeftRadius="20dp" android:topRightRadius="20dp" />
        <stroke android:width="2dp" android:color="@color/common_border_color" />
        <padding android:bottom="5dp" android:top="5dp" android:left="15dp" android:right="15dp"/>
    </shape></item>

</selector>
